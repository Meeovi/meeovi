{
   "name": "starter-template",
   "type": "module",
   "version": "1.0.0",
   "description": "This is a template for my projects.",
   "author": "Sebastian Hilton (https://www.sebastianhilton.com)",
   "homepage": "https://www.sebastianhilton.com",
   "repository": {
      "type": "git",
      "url": "git+https://github.com/Meeovi/Meeovi-Starter-Template"
   },
   "bugs": {
      "url": "https://github.com/Meeovi/Meeovi-Starter-Template/issues"
   },
   "license": "MIT",
   "private": true,
   "scripts": {
      "starter-commit": "git add . && git commit -m \"ready for update\"",
      "starter-update": "git pull \"https://github.com/Meeovi/Meeovi-Starter-Template\"",
      "build": "nuxt build",
      "dev": "nuxt dev --port 3001",
      "ts-node": "ts-node -r tsconfig-paths/register --compiler-options '{\"module\":\"CommonJS\"}'",
      "generate": "nuxt generate",
      "preview": "nuxt preview",
      "postinstall": "nuxt prepare",
      "gqlg": "npx gqlg --schemaFilePath graphql/schema.graphql --destDirPath graphql/framework --depthLimit 2"
   },
   "dependencies": {
      "@apollo/client": "^3.14.0",
      "@atproto/api": "^0.18.8",
      "@atproto/oauth-client-node": "^0.3.13",
      "@directus/sdk": "^20.1.0",
      "@directus/types": "^13.2.3",
      "@fortawesome/fontawesome-free": "^7.1.0",
      "@fortawesome/fontawesome-svg-core": "^7.1.0",
      "@fortawesome/free-solid-svg-icons": "^7.1.0",
      "@fortawesome/vue-fontawesome": "^3.1.2",
      "@mdi/font": "^7.4.47",
      "@meilisearch/instant-meilisearch": "^0.28.0",
      "@nuxt/image": "^1.11.0",
      "@nuxtjs/i18n": "^10.1.0",
      "@nuxtjs/leaflet": "^1.3.2",
      "@pinia/nuxt": "^0.11.2",
      "@simplewebauthn/browser": "^11.0.0",
      "@simplewebauthn/server": "^11.0.0",
      "@vue/apollo-composable": "^4.2.2",
      "eslint": "^9.32.0",
      "graphql": "^16.11.0",
      "graphql-tag": "^2.12.6",
      "instantsearch.css": "^8.6.0",
      "meilisearch": "^0.54.0",
      "nuxt": "^4.2.2",
      "nuxt-auth-utils": "^0.5.26",
      "nuxt-authorization": "^0.3.5",
      "nuxt-security": "^2.4.0",
      "nuxt-tiptap-editor": "^3.0.0",
      "pinia": "^3.0.3",
      "video.js": "^8.23.4",
      "vue-instantsearch": "^4.22.0",
      "vuetify-nuxt-module": "^0.18.8"
   },
   "devDependencies": {
      "@nuxtjs/turnstile": "^1.0.0",
      "@storefront-ui/nuxt": "^2.5.4",
      "@vueuse/core": "^14.0.0",
      "@vueuse/nuxt": "^14.0.0"
   }
}
