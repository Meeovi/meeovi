{
  "name": "meeovi",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "dev:base": "npm --workspace=@meeovi/app-base run dev",
    "dev:shared": "npm --workspace=@meeovi/app-shared run dev",
    "dev:dates": "npm --workspace=@meeovi/app-dates run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types"
  },
  "devDependencies": {
    "@types/mjml": "^4.7.4",
    "prettier": "^3.5.3",
    "turbo": "^2.4.4",
    "typescript": "5.8.2"
  },
  "workspaces": [
    "apps/**",
    "packages/*"
  ],
  "dependencies": {}
}
