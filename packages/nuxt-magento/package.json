{
  "name": "nuxt-magento",
  "version": "0.0.1",
  "description": "Easily integrate Magento with Nuxt 3 and 4 ðŸš€",
  "repository": "your-org/my-module",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.mjs",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/types.d.mts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "nuxt-module-build build",
    "build:template": "npm run --filter=./template build",
    "build:docs": "npm run --filter=./docs build",
    "dev": "npm run --filter=playground-v4 --env-file=.env dev",
    "dev:mock": "npm run --filter=playground-v4-mock --env-file=.env dev",
    "dev:v3": "npm run --filter=playground-v3 --env-file=.env dev",
    "dev:template": "npm run --filter=./template --env-file=.env dev",
    "dev:docs": "npm run --filter=./docs --env-file=.env dev",
    "lint": "eslint .",
    "typecheck": "vue-tsc --noEmit",
    "smoke": "node scripts/run-smoke.cjs",
    "download:schema": "curl https://meeovi.com/rest/default/schema -o runtime/types/swagger.json", 
    "convert:schema": "swagger2openapi runtime/types/swagger.json -o runtime/types/magento-oas3.json", 
    "generate:types": "openapi-typescript runtime/types/magento-oas3.json --output runtime/types/magento-schema.d.ts",
    "test": "npm run --env-file=.env vitest run",
    "test:watch": "npm run --env-file=.env vitest watch",
    "test:coverage": "vitest run --coverage",
    "analyze": "npm run --filter=playground-v4 analyze",
    "prepare": "nuxt-module-build prepare"
  },
  "dependencies": {
    "@graphql-codegen/cli": "^6.1.0",
    "@graphql-codegen/import-types-preset": "^3.0.1",
    "@graphql-codegen/introspection": "^5.0.0",
    "@graphql-codegen/plugin-helpers": "^6.1.0",
    "@graphql-codegen/typescript": "^5.0.7",
    "@nuxt/kit": "^4.2.2",
    "h3": "^1.15.4",
    "ofetch": "^1.5.1",
    "@meeovi/normalizer-core": "workspace:*"
  },
  "devDependencies": {
    "@graphql-tools/graphql-tag-pluck": "^8.3.26",
    "@nuxt/eslint-config": "^1.12.1",
    "@nuxt/module-builder": "^1.0.2",
    "@nuxt/schema": "^4.2.2",
    "@nuxt/test-utils": "^3.21.0",
    "@openapitools/openapi-generator-cli": "^2.27.0",
    "@vitest/coverage-v8": "^4.0.16",
    "eslint": "^9.39.2",
    "happy-dom": "^20.0.11",
    "nitropack": "^2.12.9",
    "nuxt": "^4.2.2",
    "openapi-typescript": "^7.10.1",
    "sharp": "^0.34.5",
    "typescript": "^5.9.3",
    "unbuild": "^3.6.1",
    "vercel": "^50.1.3",
    "vitest": "^4.0.16",
    "vue-tsc": "^3.2.1"
  },
  "keywords": [
    "nuxt",
    "nuxt3",
    "nuxt4",
    "nuxtjs",
    "nuxt-module",
    "shopify",
    "ecommerce",
    "storefront",
    "storefront-api",
    "admin-api",
    "graphql",
    "typescript",
    "codegen"
  ],
  "engines": {
    "node": "^18.x || ^20.x || ^22.x || ^24.x"
  },
  "trustedDependencies": [
    "@parcel/watcher",
    "@tailwindcss/oxide",
    "better-sqlite3",
    "esbuild",
    "nuxt-shopify-template",
    "playground",
    "playground-mock",
    "playground-v4",
    "sharp",
    "unrs-resolver",
    "vue-demi"
  ]
}
